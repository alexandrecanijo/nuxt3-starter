{
  "name": "nuxt3-starter",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "Nuxt 3 Starter Template and boilerplate with lots of features and performance in mind. (Color mode, i18n, Accessibility, Pinia, etc)",
  "author": "AlexandreCanijo.com",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
