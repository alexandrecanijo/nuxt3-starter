    "updated": "npx ncu -u"
  },
  "dependencies": {
    "nuxt": "^3.11.2",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "npm-check-updates": "^16.14.20"
  }
